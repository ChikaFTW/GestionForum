<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="557.0" prefWidth="655.0" GridPane.rowIndex="1">
            <children>
                <VBox layoutX="34.0" layoutY="14.0" prefHeight="245.0" prefWidth="88.0" spacing="32.0">
                    <children>
                        <Label text="ID" />
                        <Label text="Name" />
                        <Label prefHeight="17.0" prefWidth="72.0" text="Last Name" />
                  <Label text="Email" />
                  <Label text="Password" />
                  <Label text="phone" />
                  <Label text="birthday" />
                  <Label text="Gender" />
                  <Label text="Acces" />
                    </children>
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="14.0" />
                    </padding>
                </VBox>
                <VBox layoutX="104.0" layoutY="26.0" prefHeight="202.0" prefWidth="100.0" spacing="24.0">
                    <children>
                        <TextField fx:id="tfid" />
                        <TextField fx:id="tfName" />
                        <TextField fx:id="tfLast" />
                  <TextField fx:id="tfMail" />
                  <TextField fx:id="tfPass" />
                  <TextField fx:id="tfPhone" />
                  <DatePicker fx:id="tfBir" />
                  <ComboBox fx:id="tfGender" onAction="#Select1" prefWidth="150.0" />
                  <ComboBox fx:id="tfAcces" onAction="#Select2" prefWidth="150.0" />
                    </children>
                </VBox>
                <TableView fx:id="tvser" layoutX="205.0" layoutY="36.0" prefHeight="224.0" prefWidth="622.0">
                    <columns>
                        <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
                        <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="colLast" prefWidth="75.0" text="LastName" />
                  <TableColumn fx:id="colMail" prefWidth="75.0" text="Role" />
                  <TableColumn fx:id="colBir" prefWidth="75.0" text="Birthday" />
                  <TableColumn fx:id="colGender" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="colRole" prefWidth="75.0" text="Gender" />
                  <TableColumn fx:id="colPhone" prefWidth="75.0" text="Phone" />
                    </columns>
                    <padding>
                        <Insets left="10.0" right="10.0" />
                    </padding>
                </TableView>
                <Button fx:id="btninsert" layoutX="371.0" layoutY="499.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="67.0" text="Insert" />
                <Button fx:id="btnupdate" layoutX="453.0" layoutY="499.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="67.0" text="Update" />
                <Button fx:id="btndelete" layoutX="536.0" layoutY="499.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="67.0" text="delete" />

                <TextField fx:id="tfPass1" layoutX="300.0" layoutY="275.0" />
            </children>
        </AnchorPane>
    </children>
